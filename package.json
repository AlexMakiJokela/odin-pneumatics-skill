{
  "name": "pneumatics",
  "description": "pneumatic controls bruh",
  "version": "0.0.1",
  "skill": {
    "namespace": "pneumatics"
  },
  "homepage": "https://github.com/[YOUR REPO PATH]",
  "bugs": {
    "url": "https://github.com/[YOUR REPO PATH]/issues"
  },
  "keywords": [],
  "scripts": {
    "boot": "node build/index",
    "boot.local": "node -r ts-node/register -r tsconfig-paths/register ./src/index",
    "health": "yarn boot --health",
    "health.local": "yarn boot.local --health",
    "build.ci": "yarn build.tsc && yarn build.resolve-paths && yarn lint",
    "build.dev": "yarn build.tsc --sourceMap ; yarn resolve-paths.lint",
    "build.copy-files": "mkdir -p build && rsync -avzq --exclude='*.ts' ./src/ ./build/",
    "build.resolve-paths": "resolve-path-aliases --target build --patterns '**/*.js,**/*.d.ts'",
    "build.tsc": "yarn build.copy-files && tsc",
    "clean": "yarn clean.build",
    "clean.all": "yarn clean.dependencies && yarn clean.build",
    "clean.build": "rm -rf build/",
    "clean.dependencies": "rm -rf node_modules/ package-lock.json yarn.lock",
    "fix.lint": "eslint --fix --cache '**/*.ts'",
    "lint": "eslint --cache '**/*.ts'",
    "lint.tsc": "tsc -p . --noEmit",
    "post.watch.build": "yarn build.copy-files && yarn build.resolve-paths",
    "rebuild": "yarn clean.all && yarn && yarn build.dev",
    "update.dependencies": "yarn clean.dependencies && yarn",
    "resolve-paths.lint": "yarn build.resolve-paths ; yarn lint",
    "test": "jest",
    "watch.build.dev": "tsc-watch --sourceMap --onCompilationComplete 'yarn post.watch.build'",
    "watch.lint": "concurrently 'yarn lint' \"chokidar 'src/**/*' -c 'yarn lint.tsc'\"",
    "watch.rebuild": "yarn clean.all && yarn && yarn watch.build.dev",
    "watch.tsc": "tsc -w"
  },
  "dependencies": {
    "@sprucelabs/error": "^6.0.16",
    "@sprucelabs/globby": "^2.0.11",
    "@sprucelabs/heartwood-view-controllers": "^110.0.31",
    "@sprucelabs/mercury-client": "^42.0.32",
    "@sprucelabs/mercury-core-events": "^24.0.26",
    "@sprucelabs/mercury-types": "^47.0.22",
    "@sprucelabs/schema": "^30.0.27",
    "@sprucelabs/spruce-core-schemas": "^40.0.21",
    "@sprucelabs/spruce-event-plugin": "^62.0.23",
    "@sprucelabs/spruce-event-utils": "^40.0.24",
    "@sprucelabs/spruce-permission-plugin": "^31.0.40",
    "@sprucelabs/spruce-skill-booter": "^62.0.23",
    "@sprucelabs/spruce-skill-utils": "^31.0.28",
    "@sprucelabs/spruce-view-plugin": "^47.0.38",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@sprucelabs/jest-json-reporter": "^8.0.18",
    "@sprucelabs/resolve-path-aliases": "^2.0.15",
    "@sprucelabs/spruce-test-fixtures": "^62.0.23",
    "@sprucelabs/test": "^9.0.11",
    "@sprucelabs/test-utils": "^5.0.18",
    "@types/node": "^20.12.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.2.0",
    "eslint-config-spruce": "^11.2.15",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "yarn": "1.x"
  },
  "jest": {
    "testRunner": "jest-circus/runner",
    "maxWorkers": 4,
    "testTimeout": 120000,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/tmp/",
      "<rootDir>/src/",
      "<rootDir>/node_modules/",
      "<rootDir>/build/__tests__/testDirsAndFiles/"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)"
    ],
    "moduleNameMapper": {
      "^#spruce/(.*)$": "<rootDir>/build/.spruce/$1"
    }
  }
}
